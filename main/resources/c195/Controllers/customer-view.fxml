<AnchorPane prefHeight="500.0" prefWidth="1000.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="c195.Controllers.CustomerController">
   <children>
      <TableView fx:id="customerTable" layoutX="14.0" layoutY="80.0" prefHeight="340.0" prefWidth="820.0">
        <columns>
          <TableColumn fx:id="custIdColumn" text="Customer ID" />
          <TableColumn fx:id="custNameColumn" prefWidth="150.0" text="Customer Name" />
            <TableColumn fx:id="addressColumn" prefWidth="272.0" text="Address" />
            <TableColumn fx:id="postCodeColumn" prefWidth="93.0" text="Postal Code" />
            <TableColumn fx:id="phoneNumberColumn" prefWidth="156.0" text="Phone Number" />
            <TableColumn fx:id="divIdColumn" minWidth="0.0" prefWidth="67.0" text="Division ID" />
        </columns>
      </TableView>
      <VBox layoutX="846.0" layoutY="80.0" prefHeight="175.0" prefWidth="140.0">
         <children>
            <Button fx:id="addCustBtn" mnemonicParsing="false" onAction="#OnAddCustBtn" prefHeight="30.0" prefWidth="140.0" text="Add Customer">
               <VBox.margin>
                  <Insets bottom="20.0" />
               </VBox.margin></Button>
            <Button fx:id="modCustBtn" mnemonicParsing="false" onAction="#OnModCustBtn" prefHeight="30.0" prefWidth="140.0" text="Modify Customer">
               <VBox.margin>
                  <Insets bottom="20.0" />
               </VBox.margin></Button>
            <Button fx:id="delCustBtn" mnemonicParsing="false" onAction="#OnDelCustBtn" prefHeight="30.0" prefWidth="140.0" text="Delete Customer" />
         </children></VBox>
      <VBox layoutX="846.0" layoutY="311.0" prefHeight="175.0" prefWidth="140.0">
         <children>
            <Button fx:id="appointmentsBtn" mnemonicParsing="false" onAction="#OnAppointmentsBtn" prefHeight="30.0" prefWidth="140.0" text="Appointments">
               <VBox.margin>
                  <Insets bottom="20.0" />
               </VBox.margin></Button>
            <Button fx:id="reportsBtn" mnemonicParsing="false" onAction="#OnReportsBtn" prefHeight="30.0" prefWidth="140.0" text="Reports" />
            <Button fx:id="exitBtn" mnemonicParsing="false" onAction="#OnExitBtn" prefHeight="30.0" prefWidth="140.0" text="Exit">
               <VBox.margin>
                  <Insets top="60.0" />
               </VBox.margin></Button>
         </children></VBox>
      <Label layoutX="368.0" layoutY="14.0" text="Customers">
         <font>
            <Font size="24.0" />
         </font>
      </Label>
      <Line endX="100.0" layoutX="424.0" layoutY="49.0" startX="-100.0" />
   </children>
